{
  "name": "tesla-customer-decoder-api-gateway",
  "version": "2.0.0",
  "description": "API Gateway for Tesla Customer Decoder microservices architecture",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "health": "curl http://localhost:8080/health",
    "services": "curl http://localhost:8080/api/services"
  },
  "keywords": [
    "tesla",
    "customer-decoder",
    "api-gateway",
    "microservices",
    "nodejs",
    "express"
  ],
  "author": "TRAE AI Assistant",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "compression": "^1.7.4",
    "express-rate-limit": "^6.7.0",
    "http-proxy-middleware": "^2.0.6",
    "uuid": "^9.0.0",
    "node-fetch": "^3.3.1"
  },
  "devDependencies": {
    "nodemon": "^2.0.22",
    "jest": "^29.5.0",
    "supertest": "^6.3.3",
    "eslint": "^8.40.0"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "config": {
    "port": 8080
  },
  "jest": {
    "testEnvironment": "node",
    "collectCoverageFrom": [
      "**/*.js",
      "!node_modules/**",
      "!coverage/**"
    ]
  }
}