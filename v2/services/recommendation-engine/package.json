{
  "name": "tesla-recommendation-engine-service",
  "version": "2.0.0",
  "description": "Tesla Customer Decoder - Recommendation Engine Microservice",
  "main": "service.js",
  "scripts": {
    "start": "node service.js",
    "dev": "nodemon service.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "health": "curl -f http://localhost:3005/health || exit 1"
  },
  "keywords": [
    "tesla",
    "customer-decoder",
    "recommendation-engine",
    "microservice",
    "personalization",
    "ai"
  ],
  "author": "TRAE AI Assistant",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "nodemon": "^3.0.1",
    "jest": "^29.7.0",
    "supertest": "^6.3.3",
    "eslint": "^8.50.0"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "jest": {
    "testEnvironment": "node",
    "collectCoverageFrom": [
      "**/*.js",
      "!**/node_modules/**",
      "!**/coverage/**"
    ]
  },
  "config": {
    "port": 3005
  }
}